@model IEnumerable<RatingUniversity.Models.I18_effektivnost_provodimih_nauchnih_rabot>

@{ Layout = "~/Views/Shared/_LayoutSideMenu.cshtml"; }

<h3>@ViewBag.Title</h3>

<table class="table table-striped scroll" style="font-size:14px;">
    <thead>
    <tr>
        <th>Рейтинг</th>
        <th>Название высшего учебного заведения</th>
        <th>количество проводимых научных исследований на базе предприятий, научно-исследовательских институтов и конструкторских бюро</th>
        <th>количество полученных охранных документов (патентов) на объекты интеллектуальной собственности</th>
        <th>количество внедренных в практику научных разработок (на основе актов внедрения, подтвержденных соответствующими органами управления)</th>
        <th>количество заключенных вузом договоров,  на республиканской ярмарке инновационных идей, технологий и проектов</th>
        <th>количество лицензионных соглашений на результаты интеллектуальной собственности</th>
        <th>Эффективность проводимых научно-изыскательских работ</th>
        <th>Показатель эффективности проводимых научно-изыскательских работ</th>
        <th>Год проведения рейтинга</th>
    </tr>
    </thead>
@{var position=0;}
@foreach (var item in Model)
{
    position++;
    string name = (ViewBag.lang == "RU") ? item.name_RU : item.name_UZ;
    <tr>
        <td>
            @position
        </td>
        <td width="35%">
            @RatingUniversity.Classes.Functions.Translate(name, ViewBag.lang, ViewBag.alfabet)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.I181)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.I182)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.I183)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.I184)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.I185)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.I18)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mark)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.YEAR)
        </td>
    </tr>
}

</table>
