@model IEnumerable<RatingUniversity.Models.I19_stepen_osnashennosti_laboratoriy_oborudovaniem>

@{ Layout = "~/Views/Shared/_LayoutSideMenu.cshtml"; }

<h3>@ViewBag.Title</h3>

<table class="table table-striped" style="font-size:14px;">
    <tr>
        <th>Рейтинг</th>
        <th>Название высшего учебного заведения</th>
        <th>Степень оснащенности научной экспериментально-лабораторной базы современным оборудованием</th>
        <th>Показатель cтепени оснащенности научной экспериментально-лабораторной базы современным оборудованием</th>
        <th>Год проведения рейтинга</th>
    </tr>

@{var position=0;}
@foreach (var item in Model)
{
    position++;
    <tr>
        <td>
            @position
        </td>
        <td width="35%">
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.I19)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mark)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.YEAR)
        </td>
    </tr>
}

</table>
