@model IEnumerable<RatingUniversity.Models.Jadval11>
@{
	Layout = "~/Views/Shared/_LayoutSideMenu.cshtml";
}

<h2>Universitetlar reytingi</h2>

<h3>Download File</h3>

<a href="/Jadval11/Download">Yuklab olish</a>


<h3>Upload File</h3>

@using (Html.BeginForm("Upload", "Jadval11", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
	<input type="file" name="files" />
	<input type="submit" name="Submit" id="Submit" value="Upload" />
}

@if (ViewBag.bor == true)
{
	<h3>Ma'lumotlar:</h3>
	<table class="table">
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.FullName)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Speciality)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.MonographName)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.MongographYear)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.DarslikName)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.DarslikCertificate)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.OquvqullanmaName)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.OquvqullanmaCertificate)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.OquvmajmuaName)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.OquvmajmuaCertificate)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Year)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.UniversityId)
			</th>
		</tr>

		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.FullName)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Speciality)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.MonographName)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.MongographYear)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.DarslikName)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.DarslikCertificate)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.OquvqullanmaName)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.OquvqullanmaCertificate)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.OquvmajmuaName)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.OquvmajmuaCertificate)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Year)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.UniversityId)
				</td>
			</tr>
		}

	</table>
}