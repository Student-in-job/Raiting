@model IEnumerable<RatingUniversity.Models.I9_dolya_pps_prepodayushego_v_sspo>

@{ Layout = "~/Views/Shared/_LayoutSideMenuEdit.cshtml"; }

<h3>@ViewBag.Title</h3>

<table class="table table-striped" style="font-size:14px;">
    <tr>
        <th>Рейтинг</th>
        <th>Название высшего учебного заведения</th>
        <th>Численность ППС, проводящего учебные занятия в учреждениях ССПО</th>
        <th>Показатель численность ППС, проводящего учебные занятия в учреждениях ССПО</th>
        <th>Год проведения рейтинга</th>
    </tr>

@{var position=0;}
@foreach (var item in Model)
{
    position++;
    <tr>
        <td>
            @position
        </td>
        <td width="35%">
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.I9)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mark)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.YEAR)
        </td>
    </tr>
}

</table>
