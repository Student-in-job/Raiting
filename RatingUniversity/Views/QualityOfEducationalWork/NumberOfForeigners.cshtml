@model IEnumerable<RatingUniversity.I4_dolya_inostrannih_prepodavateley_i_studentov>

@{ Layout = "~/Views/Shared/_LayoutSideMenuEdit.cshtml"; }

<h3>@ViewBag.Title</h3>

<table class="table table-striped" style="font-size:14px;">
    <tr>
        <th>Рейтинг</th>
        <th>Название высшего учебного заведения</th>
        <th>Доля иностранных преподавателей</th>
        <th>Доля иностранных студентов</th>
        <th>Доля иностранных преподавателей и иностранных студентов</th>
        <th>Год проведения рейтинга</th>
    </tr>

@{var position = 0;}
@foreach (var item in Model)
{
    position++;
    <tr>
        <td>
            @position
        </td>
        <td width="35%">
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.I41)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.I42)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.YEAR)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.YEAR)
        </td>
    </tr>
}

</table>
